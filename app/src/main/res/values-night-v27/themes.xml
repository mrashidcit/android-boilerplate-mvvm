<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="SplashTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@color/gray_E9EEF1_black_1D1D1D</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarColor">@color/gray_E9EEF1_black_1D1D1D</item>
    </style>

    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/gray_E9EEF1_black_1D1D1D</item>
        <item name="colorPrimaryVariant">@color/gray_E9EEF1_black_1D1D1D</item>
        <item name="colorOnPrimary">@color/black_1D1D1D_white_FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/gray_E9EEF1_black_1D1D1D</item>
        <item name="colorSecondaryVariant">@color/gray_E9EEF1_black_1D1D1D</item>
        <item name="colorOnSecondary">@color/black_1D1D1D_white_FFFFFF</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarColor">@color/gray_E9EEF1_black_1D1D1D</item>
        <item name="android:windowLayoutInDisplayCutoutMode">shortEdges</item>
        <item name="android:windowBackground">@color/gray_E9EEF1_black_1D1D1D</item>
    </style>

</resources>