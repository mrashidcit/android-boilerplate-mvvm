<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="SplashTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@color/white_FFFFFF_black_101010</item>
        <item name="android:navigationBarColor">@color/white_FFFFFF_black_101010</item>
    </style>

    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_FF6200EE_black_101010</item>
        <item name="colorPrimaryVariant">@color/purple_FF3700B3_black_101010</item>
        <item name="colorOnPrimary">@color/white_FFFFFF_black_201E1E</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_FF03DAC5</item>
        <item name="colorSecondaryVariant">@color/teal_FF018786</item>
        <item name="colorOnSecondary">@color/black_000000</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

</resources>